#include <iostream>
using namespace std;

int main()
{
    int n;
    cin >> n;
    while (n--)
    {
        char chu;
        cin >> chu;
        if (chu >= 'a' && chu <= 'z')
        {
            chu = toupper(chu);
        }
        else if (chu >= 'A' && chu <= 'Z')
        {
            chu = tolower(chu);
        }
        cout << chu << endl;
    }
}
